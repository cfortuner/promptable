{
  "name": "root",
  "private": true,
  "workspaces": [
    "apps/*",
    "starter-kits/*",
    "packages/*",
    "examples"
  ],
  "scripts": {
    "build": "FORCE_COLOR=1 NODE_ENV=development turbo run build",
    "dev": "FORCE_COLOR=1 turbo run dev --parallel",
    "lint": "FORCE_COLOR=1 turbo run lint",
    "test": "FORCE_COLOR=1 turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "publish-packages": "FORCE_COLOR=1 turbo run build lint test --filter promptable && changeset version && changeset publish",
    "example": "FORCE_COLOR=1 pnpm --filter examples run start",
    "example-latest": "FORCE_COLOR=1 pnpm --filter examples-latest run start"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.0",
    "@manypkg/cli": "^0.20.0",
    "eslint-config-custom": "0.0.0",
    "prettier": "latest",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.18.2",
  "keywords": [
    "llm",
    "langchain",
    "ai",
    "gptindex",
    "gpt3",
    "chain",
    "prompt",
    "prompt programming",
    "promptable",
    "nlp",
    "javascript",
    "react",
    "chatgpt",
    "model",
    "machine learning",
    "ml",
    "typescript"
  ]
}
