{
  "name": "cli",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "start": "node dist/index.mjs",
    "dev": "rm -rf dist && tsup ./src/index.ts --format=esm --watch --dts --external react",
    "lint": "TIMING=1 eslint \"**/*.ts*\"",
    "lint:ci": "yarn run lint",
    "build": "rm -rf dist && tsup ./src/index.ts --format=esm --dts --external react",
    "build:ci": "yarn run build"
  },
  "bin": {
    "app-cli": "./dist/index.mjs"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "commander": "^9.4.1",
    "dotenv": "^16.0.3",
    "promptable": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "eslint": "7.32.0",
    "ts-node": "^10.9.1",
    "tsconfig": "0.0.0"
  }
}
